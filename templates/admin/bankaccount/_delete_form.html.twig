<form method="post" action="{{ path('admin_bankaccount_delete', {'id': bankaccount.id}) }}" onsubmit="Voulez-vous vraiment supprimer ce compte?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ bankaccount.id) }}">
